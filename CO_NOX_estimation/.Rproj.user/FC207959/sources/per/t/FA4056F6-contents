ds = read.csv("Gas_turbine/gt_2011.csv")
attach(ds)
dev.new()
pairs(ds)
summary(ds)

# Correlation-matrix
library(corrplot)
library(RColorBrewer)
cor <-cor(ds)
dev.new()
corrplot(cor, type="upper", order="hclust",col=brewer.pal(n=8, name="RdYlBu"))


fit = lm(CO~., data = ds)
summary(fit)
library(car)
vif(fit)
  
dev.new()
par(mfrow = c(2,2))
plot(fit)
